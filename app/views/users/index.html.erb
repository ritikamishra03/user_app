<h1>All Users</h1>
<table>
    <tr>
        <th>name <hr></th>
        <th>email <hr></th>
        <th><%= link_to 'New User', new_user_path %><hr></th>
    </tr>

    <% @users.each do |user| %>
    <tr>        
        <td><%=user.name%></td>
        <td><%=user.email%></td>
    </tr>
    <%end%>

</table>    

